{
  "name": "travel",
  "version": "1.0.0",
  "description": "",
  "author": "Radu Proca",
  "license": "",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "docker-compose -f ./test/docker-compose.yml up --build --force-recreate --renew-anon-volumes --abort-on-container-exit --exit-code-from travels-e2e-test",
    "test:e2e:run": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.3.1",
    "@nestjs/common": "^6.11.1",
    "@nestjs/config": "0.1.0",
    "@nestjs/core": "^6.11.1",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/mongoose": "^6.2.1",
    "@nestjs/passport": "^6.1.1",
    "@nestjs/platform-express": "^6.11.1",
    "@nestjs/swagger": "^4.2.0",
    "@nestjs/terminus": "^6.5.5",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "cors": "^2.8.5",
    "helmet": "^3.21.2",
    "moment": "^2.24.0",
    "mongoose": "^5.8.9",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.3",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.14.1",
    "@nestjs/schematics": "^6.8.2",
    "@nestjs/testing": "^6.11.1",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.9.1",
    "@types/mongoose": "^5.5.43",
    "@types/node": "^13.5.0",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/passport-local-mongoose": "^4.0.13",
    "@types/randomstring": "^1.1.6",
    "@types/uuid": "^3.4.6",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "form-data": "^3.0.0",
    "jest": "^25.1.0",
    "jison": "^0.4.18",
    "randomstring": "^1.1.5",
    "rimraf": "^3.0.0",
    "ts-jest": "^25.0.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFiles": []
  }
}
